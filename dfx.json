{
  "canisters": {
    "vetkd_system_api": {
      "candid": "vetkd_system_api.did",
      "type": "custom",
      "wasm": "vetkd_system_api.wasm"
    },
    "backend": {
      "dependencies": [
        "vetkd_system_api"
      ],
      "main": "src/backend/main.mo",
      "type": "motoko"
    },
    "ckbtc_ledger" : {
      "type" : "custom",
      "candid" : "ledger.did",
      "wasm" : "ledger_canister.wasm"
    },
    "frontend": {
      "dependencies": [
        "backend",
        "vetkd_system_api",
        "relay"
      ],
      "frontend": {
        "entrypoint": "src/frontend/public/index.html"
      },
      "source": [
        "src/frontend/public/"
      ],
      "type": "assets"
    },
    "internet_identity": {
      "candid": "internet_identity.did",
      "type": "custom",
      "wasm": "internet_identity.wasm"
    },
    "relay": {
      "candid": "src/relay/relay.did",
      "type": "rust",
      "package": "relay"
    },
    "foreign_relay": {
      "candid": "src/relay/relay.did",
      "type": "rust",
      "package": "foreign_relay"
    }
  },
  "dfx": "0.14.2",
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
