{
  "canisters": {
    "vetkd_system_api": {
      "candid": "vetkd_system_api.did",
      "type": "custom",
      "wasm": "vetkd_system_api.wasm"
    },
    "candb_file_upload": {
      "main": "src/candb_file_upload/main.mo",
      "type": "motoko"
    },
    "backend": {
      "dependencies": [
        "vetkd_system_api",
        "dynamic_relays"
      ],
      "main": "src/backend/main.mo",
      "type": "motoko"
    },
    "ckbtc_ledger" : {
      "type" : "custom",
      "candid" : "ledger.did",
      "wasm" : "ledger_canister.wasm"
    },
    "frontend": {
      "dependencies": [
        "backend",
        "internet_identity"
      ],
      "frontend": {
        "entrypoint": "src/frontend/public/index.html"
      },
      "source": [
        "src/frontend/public/"
      ],
      "type": "assets"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2023-02-12_2/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2023-02-12_2/internet_identity_dev.wasm",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    },
    "dynamic_relays": {
      "candid": "src/dynamic_relays/dynamic_relays.did",
      "type": "rust",
      "package": "dynamic_relays",
      "dependencies": []
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "dfx": "0.14.2",
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
